  emailsent = false;
  TrHs = true;
  IVr.OrderNr = ORr.SerNr;
  while (LoopKey("OrderNr",IVr,1,TrHs)) begin
    if (IVr.OrderNr!=ORr.SerNr) then begin
      TrHs = false;
